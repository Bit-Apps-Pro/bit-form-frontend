    "autoprefixer": "^10.2.6",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "compression-webpack-plugin": "^8.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
